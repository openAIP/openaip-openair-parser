{
    "name": "@openaip/openair-parser",
    "version": "2.12.0",
    "description": "OpenAIR format parser. Parses openAIR formatted string and outputs it in JSON or GeoJSON.",
    "keywords": [
        "openaip",
        "openair",
        "parser",
        "json",
        "geojson"
    ],
    "authors": [
        {
            "name": "Stephan Besser",
            "email": "stephan@openaip.net",
            "homepage": "https://www.openaip.net",
            "role": "Lead developer"
        }
    ],
    "engines": {
        "node": ">=24.0.0"
    },
    "type": "module",
    "main": "./dist/index.js",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.js",
            "default": "./dist/*.js"
        }
    },
    "files": [
        "dist"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/openAIP/openaip-openair-parser.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/"
    },
    "bugs": {
        "url": "https://github.com/openAIP/openaip-openair-parser/issues"
    },
    "homepage": "https://github.com/openAIP/openaip-openair-parser#readme",
    "scripts": {
        "test": "vitest run",
        "build": "npm run tsc:clean && npm run tsc:build",
        "tsc:run": "tsc --noEmit",
        "tsc:clean": "tsc --build --clean",
        "tsc:build": "tsc --build",
        "code:format": "npx @biomejs/biome format --write",
        "code:lint": "npx @biomejs/biome lint --write",
        "code:check": "npx @biomejs/biome check --write",
        "release:patch": "npm version --sign-git-tag patch && git push && git push origin --tags",
        "release:minor": "npm version --sign-git-tag minor && git push && git push origin --tags",
        "release:major": "npm version --sign-git-tag major && git push && git push origin --tags",
        "run-security-audit": "npm audit --omit=dev",
        "update-packages": "ncu --target minor --upgrade && npm install && npm audit fix",
        "depcheck": "npx depcheck --oneline --skip-missing=true --ignore-patterns=dist,coverage,*.log"
    },
    "dependencies": {
        "@openaip/coordinate-parser": "^0.31.0",
        "@placemarkio/check-geojson": "^0.1.14",
        "@turf/turf": "^7.3.1",
        "@types/sprintf-js": "^1.1.4",
        "commander": "^13.1.0",
        "sprintf-js": "^1.1.3",
        "uuid": "^13.0.0",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@biomejs/biome": "2.3.8",
        "@types/node": "^24.10.4",
        "@vitest/coverage-v8": "^4.0.15",
        "app-root-path": "^3.1.0",
        "depcheck": "^1.4.7",
        "npm-check-updates": "^19.2.0",
        "typescript": "^5.9.3",
        "vite": "^7.3.0",
        "vitest": "^4.0.15"
    },
    "comments": {
        "packages": {
            "zod": "Version 4 introduces breaking changes!"
        }
    }
}
